<!DOCTYPE html>
<html>
    <head>
        <meta charset='utf-8'>
        <title>Index</title>
        <link rel="stylesheet" href="~/css/style1.css"/>
    </head>
    <body>
        <div class="msg">
            <ul>
                @foreach (var x in @ViewBag.all){
                    <li>
                        <h3>Review for: @x.rname</h3>
                        @x.rating star
                        <h5>Reviewer: @x.uname</h5>
                        @x.date
                        <p>@x.comment</p>
                        <hr>
                    </li>
                }
            </ul>
            
           
        </div>
    </body>
</html>

